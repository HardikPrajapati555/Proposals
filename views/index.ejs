    <body style="background-color: #F1FAFF ;">
       

        <h1 id="openFormBtn2" style="margin-top: -700px;"><b>PROPOSALS </b><i class="fa-solid fa-circle-plus"></i></h1>





        <div id="proposalModal" class="modal">
            <div class="modal-content">
                <span class="close">&times;</span>

                <!-- Step 1 -->
                <div id="formStep1" style=" font-family: 'Nunito Sans';">
                    <h4>CREATE PROPOSAL</h4>
                    <hr>
                    <form id="proposalFormStep1">
                        <div class="form-inner">
                            <h6 style="border:"none";>Client Name</h6>
                            <input type="text" name="clientName">
                        </div>

                        <div class="date-time-record">
                            <div class="duration">
                                <h6>Duration: Starts on</h6>
                                <input type="date" name="startDate">
                            </div>
                            <div class="duration">
                                <h6>Runs for</h6>
                                <div class="date-time">
                                    <input type="date" name="duration">
                                    <h6>Days</h6>
                                </div>
                            </div>
                            <div class="duration">
                                <h6>Slot Duration</h6>
                                <div class="date-time">
                                    <input type="number" name="slotDuration" id="seconds" class="seconds-input" min="0"
                                        max="59" step="1">
                                    <h6>Sec</h6>
                                </div>
                            </div>
                        </div>
                        <div class="city-dropdown">
                            <h6>Cities</h6>
                            <select name="cities" id="city">
                                <option value="Ahmedabad">Ahmedabad</option>
                                <option value="Gandhinagar">Gandhinagar</option>
                                <option value="Surat">Surat</option>
                            </select>
                        </div>

                        <div class="city-dropdown">
                            <h6>Client Type</h6>
                            <select name="clientType" id="">
                                <option value="Ahmedabad">National Agency</option>
                                <option value="Gandhinagar">Local Agency</option>
                                <option value="Surat">Direct Client(National)</option>
                            </select>
                        </div>
                        <div class="city-dropdown">
                            <h6>Property Type</h6>
                            <select name="propertyType" id="">
                                <option value="Ahmedabad">Residential</option>
                                <option value="Gandhinagar">Corporate</option>
                                <option value="Surat">Commercial</option>
                            </select>
                        </div>
                        <div class="city-dropdown">
                            <h6>Plan</h6>
                            <select name="plan" id="">
                               
                                <option value="Gandhinagar">Standerd</option>
                                <option value="Surat">Primium</option>
                            </select>
                        </div>
                        <div class="form-inner">
                            <h6>Advertisier Tag</h6>
                            <input type="text" name="advertiserTag">
                        </div>
                        <div class="toggle-btn">
                            <h6>POP Required?</h6>
                            <label class="switch">
                                <input type="checkbox" name="popRequired">
                                <span class="slider"></span>
                            </label>
                        </div>
                        <div class="toggle-btn">
                            <h6>Geo Tagging Required?</h6>
                            <label class="switch">
                                <input type="checkbox" name="geoTaggingRequired">
                                <span class="slider"></span>
                            </label>
                        </div>
                        <button type="button" id="nextStepBtn">Next</button>
                    </form>
                </div>

                <!-- Step 2 -->
                <div id="formStep2" style="display: none;">

                    <h4>UPLOAD MEDIA</h4>
                    <hr>
                    <div class="form" style=" font-family: 'Nunito Sans';">
                        <style>

                        </style>
                        <div class="img-video">
                            <h6>Type:</h6>
                            <div class="type-button">
                                <button class="dark">Image/Gif</button>
                                <button>Videos</button>
                                <button>Web</button>
                            </div>
                        </div>
                        <div class="form-inner">
                            <h6 style="border: none; font-family: " Gilroy-Medium";>File Name:</h6>
                            <input type="text" name="fileName">
                        </div>
                        <div class="upload-area" style="background-size: cover;">
                            <h6 style="    margin-left: -139px;">Media:</h6>
                            <button class="upload-button" id="uploadButton"><img src="/SVG/Uplod.svg" alt="">Click To
                                Upload</button>
                            <input type="file" id="fileInput" style="display: none;">
                        </div>
                        <div class="preview-container" id="previewContainer"
                            style="display: none;      margin: -9px -431px;">
                            <img id="previewImage" src="/SVG/aekads.svg" alt="Image Preview">
                            <button class="delete-button" id="deleteButton"><img src="/SVG/Delete.svg" alt=""></button>
                        </div>
                        <div class="Creative-Instruction">
                            <h6>Creative Instruction</h6>
                            <input type="text" name="creativeInstruction">
                            <div class="edit-next-btn2">
                                <button id="goBackBtn" class="edit-btn12"
                                    style="background-color:rgba(232, 175, 4, 1) ;">Go Back</button>
                                <button id="nextBtnStep2" class="next-btn2">Next</button>
                            </div>
                        </div>
                    </div>
                </div>

