        <div class="table3-container34">
            <table class="table10">
                <thead>
                    <tr>
                        <th>
                            <button class="btn30" onclick="showDraftTable()">Draft</button>
                        </th>
                        <th style="margin-left:20px ;">
                            <button class="btn31" style="margin-left:10px ;">Pending Activation</button>
                        </th>
                        <th>
                            <button class="btn32">Active</button>
                        </th>
                        <th>
                            <button class="btn33" onclick="showDeletedProposals()">Deleted</button>
                        </th>
                    </tr>
                </thead>
            </table>


            <div class="searchbar" style="margin: 22px -9px; display: flex; align-items: center;">
                <i class="fa-solid fa-magnifying-glass" style="margin-right: 1px;color: #999696;"></i>
                <input type="text" placeholder="Search" style="width: 700px; margin: -20px 1px; background-color: #E6EEF3; border-radius: 5px; padding-left: 10px;">
            </div>
            

            <!-- <h2>Saved Proposals</h2> -->
            <div class="table-container" id="main-proposals-table" style="border-radius: 20px;">
                <table style="width: 690px; border-radius: 20px;">
                    <thead>
                        <tr style="height:30px; background-color: white; gap: 2px;">
                            <th>PROPOSAL NAME</th>
                            <th>SUMMARY</th>
                            <th>REACH</th>
                            <th>ACTION</th>
                        </tr>
                    </thead>
                    <tbody style="background-color: white; width: 600px;font-family: Nunito sans-serif;">
                        <% proposals.forEach(proposal=> { %>
                            <% if (!proposal.deleted) { %>
                                <tr>
                                    <td>
                                        <%= proposal.clientName %>
                                    </td>
                                    <td>
                                        <%= proposal.slotDuration %>
                                    </td>
                                    <td>
                                        <%= proposal.cities.join(', ') %></td>
                                <td>
                                    <button style="    width: 100px;
                                    border: none;
                                    border-radius: 9px;
                                    background-color: #0D6EFD;
                                    color: white;" class="submitted-button" data-id="<%= proposal._id %>">Submit</button>
                                    <hr style="background-color: white;">
                                    <button style="color: #EF8E8F; margin: 1px 18px;" class="discard-button" data-id="<%= proposal._id %>">Discard</button>
                                </td>
                            </tr>
                        <% } %>
                    <% }) %>
                </tbody>
            </table>
        </div>



        <!-- deleted  -->
    <div class="table-container" id="deleted-proposals-table" style="border-radius: 20px; display: none;">
        <table style="width: 690px; border-radius: 20px;">
            <thead>
                <tr style="height:30px; background-color: white; gap: 2px;">
                    <th>PROPOSAL NAME</th>
                    <th>SUMMARY</th>
                    <th>REACH</th>
                    <th>ACTION</th>
                </tr>
            </thead>
            <tbody style="background-color: white; width: 600px;">
                <% proposals.forEach(proposal => { %>
                    <% if (proposal.deleted) { %>
                        <tr>
                            <td><%= proposal.clientName %></td>
                            <td><%= proposal.slotDuration %></td>
                            <td><%= proposal.cities.join(' , ') %></td>
                            <td>
                                <button style="border: none; border-radius: 4px; background-color: #0D6EFD; color: white;" class="submitted-button" data-id="<%= proposal._id %>">Submitted</button>
                                <hr style="background-color: white;">
                                <button style="color: #EF8E8F; margin: 1px 1px;" class="discard-button" data-id="<%= proposal._id %>">Discard</button>
                            </td>
                        </tr>
                    <% } %>
                <% }) %>
            </tbody>
        </table>
    </div>


      <script src="/scripts.js"></script>
  </body>
